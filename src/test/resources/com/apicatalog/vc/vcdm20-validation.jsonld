{
  "@context": [
    "context.jsonld",
    {
      "@base": "verifier-manifest"
    }
  ],
  "@id": "",
  "@type": "mf:Manifest",
  "name": "VCDM 2.0 Validation",
  "sequence": [
    {
      "@id": "#t0001",
      "@type": [
        "PositiveEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid unsigned VC",
      "input": "vcdm20/validation/vcdm20-valid-unsigned-vc.jsonld"
    },
    {
      "@id": "#t0002",
      "@type": [
        "PositiveEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid unsigned VP with VC2.0",
      "input": "vcdm20/validation/vcdm20-valid-unsigned-vp-vc.jsonld"
    },
    {
      "@id": "#t0003",
      "@type": [
        "PositiveEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid unsigned VP with VC1.0",
      "input": "vcdm20/validation/vcdm20-valid-unsigned-vp-vc11.jsonld",
      "expectErrorCode": "MissingProof"
    },
    {
      "@id": "#t0004",
      "@type": [
        "PositiveEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid unsigned VP with EVC",
      "input": "vcdm20/validation/vcdm20-valid-unsigned-vp-evc.jsonld"
    },
    {
      "@id": "#t0005",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid EVP",
      "input": "vcdm20/validation/vcdm20-valid-evp.jsonld",
      "expectErrorCode": "UnsupportedOperationException"
    },
    {
      "@id": "#t0006",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "@context only",
      "input": "vcdm20/validation/vcdm20-context-only.jsonld",
      "expectErrorCode": "InvalidDocument"
    },
    {
      "@id": "#t0007",
      "@type": [
        "PositiveEvaluationTest",
        "ValidationTest"
      ],
      "name": "Valid unsigned LangMaps",
      "input": "vcdm20/validation/vcdm20-valid-unsigned-langmap.jsonld"
    },
    {
      "@id": "#t0008",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "Missing @type",
     "input": "vcdm20/validation/vcdm20-missing-type.jsonld",
      "expectErrorCode": "MissingType"
    },
    {
      "@id": "#t0009",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "Unknown @type",
      "input": "vcdm20/validation/vcdm20-unknown-type.jsonld",
      "expectErrorCode": "UnknownType"
    },
    {
      "@id": "#t00010",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "Missing subject",
      "input": "vcdm20/validation/vcdm20-missing-subject.jsonld",
      "expectErrorCode": "MissingCredentialSubject"
    },
    {
      "@id": "#t00011",
      "@type": [
        "NegativeEvaluationTest",
        "ValidationTest"
      ],
      "name": "Missing subject claims",
      "input": "vcdm20/validation/vcdm20-missing-subject-claims.jsonld",
      "expectErrorCode": "MissingCredentialSubjectClaims"
    }    
  ]
}