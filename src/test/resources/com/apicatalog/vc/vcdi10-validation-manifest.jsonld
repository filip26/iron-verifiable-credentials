{
  "@context": [
    "context.jsonld",
    {
      "@base": "verifier-manifest"
    }
  ],
  "@id": "",
  "@type": "mf:Manifest",
  "name": "VC Data Integrity Tests",
  "sequence": [
    {
      "@id": "#t0001",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Valid minimal DI",
      "input": "vcdi10/vcdi10-valid-minimal.jsonld"
    },
    {
      "@id": "#t0002",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Valid minimal DI VC1.1",
      "input": "vcdi10/vcdi10-valid-minimal-vc11.jsonld"
    },
    {
      "@id": "#t0003",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Valid minimal DI VC2.0",
      "input": "vcdi10/vcdi10-valid-minimal-vc20.jsonld"
    },
    {
      "@id": "#t0010",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Missing proofPurpose",
      "input": "vcdi10/vcdi10-missing-purpose.jsonld",
      "expectErrorCode": "MissingProofPurpose"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid proofPurpose",
      "input": "vcdi10/vcdi10-invalid-purpose.jsonld",
      "expectErrorCode": "InvalidProofPurpose"
    },
    {
      "@id": "#t0028",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert proofPurpose match",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "purpose": "https://w3id.org/security#assertionMethod"
      }
    },
    {
      "@id": "#t0029",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert proofPurpose fail",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "purpose": "https://w3id.org/security#invalidMethod"
      },
      "expectErrorCode": "InvalidProofPurpose"
    },
    
    {
      "@id": "#t0015",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Missing proofValue",
      "input": "vcdi10/vcdi10-missing-signature.jsonld",
      "expectErrorCode": "MissingProofValue"
    },
    {
      "@id": "#t0016",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid proofValue",
      "input": "vcdi10/vcdi10-invalid-signature.jsonld",
      "expectErrorCode": "InvalidProofValue"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Missing cryptosuite",
      "input": "vcdi10/vcdi10-missing-crypto.jsonld",
      "expectErrorCode": "MissingCryptosuite"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Unknown cryptosuite",
      "input": "vcdi10/vcdi10-unknown-crypto.jsonld",
      "expectErrorCode": "UnknownCryptosuite"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid Created",
      "input": "vcdi10/vcdi10-invalid-created.jsonld",
      "expectErrorCode": "InvalidCreated"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid Expires",
      "input": "vcdi10/vcdi10-invalid-expires.jsonld",
      "expectErrorCode": "InvalidExpires"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid Validity Period",
      "input": "vcdi10/vcdi10-invalid-period.jsonld",
      "expectErrorCode": "InvalidValidityPeriod"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Missing Method Id",
      "input": "vcdi10/vcdi10-missing-method-id.jsonld",
      "expectErrorCode": "MissingVerificationMethodId"
    },
    {
      "@id": "#t0014",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Invalid Method Id",
      "input": "vcdi10/vcdi10-invalid-method-id.jsonld",
      "expectErrorCode": "InvalidId"
    },
    {
      "@id": "#t0023",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert domain match",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "domain": "b31d37d4-dd59-47d3-9dd8-c973da43b63a"
      }
    },
    {
      "@id": "#t0024",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert domain fail",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "domain": "4jt"
      },
      "expectErrorCode": "InvalidDomain"
    },
    {
      "@id": "#t0025",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert challenge match",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "challenge": "1235abcd6789"
      }
    },
    {
      "@id": "#t0026",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Assert challenge fail",
      "input": "vcdi10/vcdi10-valid-full.jsonld",
      "options": {
        "challenge": "3546568678678"
      },
      "expectErrorCode": "InvalidChallenge"
    }
  ]
}