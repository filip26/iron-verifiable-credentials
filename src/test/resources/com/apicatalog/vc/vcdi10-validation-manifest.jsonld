    },


    {
      "@id": "#t0010",
      "@type": [
        "NegativeEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Missing proofPurpose",
      "input": "vcdi10/vcdi10-missing-purpose-vc20.jsonld",
