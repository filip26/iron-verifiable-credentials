  "sequence": [
    {
      "@id": "#t0001",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Valid minimal DI",
      "input": "vcdi10/vcdi10-valid-minimal.jsonld"
    },  
    {
      "@id": "#t0002",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
      "name": "Valid minimal DI VC1.1",
      "input": "vcdi10/vcdi10-valid-minimal-vc11.jsonld"
    },
    {
      "@id": "#t0003",
      "@type": [
        "PositiveEvaluationTest",
        "ProofValidationTest"
      ],
