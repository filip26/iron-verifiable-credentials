      "expectErrorCode": "InvalidProofValue"
    },
  
    {
      "@id": "#t0007",
      "@type": [
        "NegativeEvaluationTest",
        "VeriferTest"
      ],
      "name": "invalid verificationMethod property",
      "input": "verifier/0007-in.jsonld",
      "expectErrorCode": "UnknownVerificationMethod"
    },
    {
      "@id": "#t0008",
      "@type": [
        "PositiveEvaluationTest",
